<Window x:Class="TestTask2.EditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestTask2"
        mc:Ignorable="d"
        Title="EditWindow" Height="110" Width="400">
    <Grid>
        <StackPanel>
            <TextBlock Text="New name"></TextBlock>
            <TextBox Name="NewName"></TextBox>
            <Button Content="Rename" Click="RenameButtonClick"></Button>
            <Button Content="Cancel" Click="CancelClick"></Button>
        </StackPanel>
    </Grid>
</Window>
